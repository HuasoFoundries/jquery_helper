{
  "name": "jquery_helper",
  "version": "1.3.1",

  "main": "jquery_helper.js",
  "description": "This helper bundles jQuery library plus all the jQuery and jQuery-UI plugins our app uses  in a single monolithic file.",
  "directories": {
    "example": "example"
  },

  "devDependencies": {
    "grunt": "^1",
    "grunt-contrib-connect": "^0.11.2",
    "jspm": "^0.17.0-beta.14",
    "eslint": "^1.10.3",
    "babel-eslint": "^6.0.3"
  },
  "scripts": {
    "test": "make test"
  },
  "keywords": [
    "jquery",
    "jqueryui",
    "bundle",
    "systemjs",
    "jspm"
  ],
  "author": "amenadiel@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HuasoFoundries/jquery_helper.git"
  },
  "bugs": {
    "url": "https://github.com/HuasoFoundries/jquery_helper/issues"
  },
  "homepage": "https://github.com/HuasoFoundries/jquery_helper#readme",
  "jspm": {
    "format": "amd",
    "registry": "github",
    "main": "jquery_helper.js",
    "directories": {
      "lib": "dist"
    },

    "devDependencies": {
      "hammerjs": "hammerjs/hammer.js@^2.0.8",
      "velocity": "julianshapiro/velocity@^1.2",
      "jquery": "npm:jquery@^2.2.3",
      "jquery-ui": "components/jqueryui@^1.11.4",
      "jquery-csv": "evanplaice/jquery-csv@~0.8",
      "jquery-serializejson": "marioizquierdo/jquery.serializeJSON@~2.7",
      "jquery.cookie": "carhartl/jquery-cookie@^1.4.1",
      "jquery.waitforChild": "huasofoundries/jquery.waitforChild@^1"
    },
    "overrides": {
      "github:components/jqueryui@1.11.4": {
        "main": "jquery-ui.js",
        "registry": "jspm",
        "dependencies": {
          "jquery": "*"
        }
      },
      "npm:jquery@2.2.3": {
        "format": "amd"
      }
    }
  }
}
